        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankASystem.Views"
        mc:Ignorable="d"
        Title="BankAccountWindow" Height="450" Width="600" ResizeMode="NoResize">
